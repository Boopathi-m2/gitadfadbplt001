			"usecase_id": {
				"type": "string",
				"defaultValue": "3"
			},
			"phase_id": {
				"type": "string",
				"defaultValue": "3"
			},
			"sequence_id": {
				"type": "string",
				"defaultValue": "1"
			},
			"process_id": {
				"type": "string",
				"defaultValue": "9"
			}
		},
		"variables": {
