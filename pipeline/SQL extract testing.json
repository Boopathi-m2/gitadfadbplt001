{
	"name": "pipeline1",
	"properties": {
		"activities": [
			{
				"name": "capturesqlrds_to_adls",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "AmazonRdsForSqlServerSource",
						"sqlReaderQuery": "SELECT count(*)\r\nFROM bpanorama.bpwarehouse.GBA_QSD_Market\r\nwhere region='APAC';",
						"queryTimeout": "02:00:00",
						"isolationLevel": "ReadCommitted",
						"partitionOption": "None"
					},
					"sink": {
						"type": "DelimitedTextSink",
						"storeSettings": {
							"type": "AzureBlobFSWriteSettings",
							"maxConcurrentConnections": 0
						},
						"formatSettings": {
							"type": "DelimitedTextWriteSettings",
							"quoteAllText": true,
							"fileExtension": ".txt"
						}
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"typeConversion": true,
						"typeConversionSettings": {
							"allowDataTruncation": true,
							"treatBooleanAsNumber": false
						}
					}
				},
				"inputs": [
					{
						"referenceName": "AmazonRdsForCapturesqlrds",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "sql_server_sink_compressed",
						"type": "DatasetReference",
						"parameters": {
							"container": "asp",
							"path": "dev/Regional_Market_Share/",
							"file": "GBA_QSD_Market_data_extraction"
						}
					}
				]
			}
		],
		"annotations": []
	}
}