{
	"name": "runSQLtoExcel",
	"properties": {
		"activities": [
			{
				"name": "performancesql_load_to_adls_copy",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "SnowflakeSource",
						"query": {
							"value": "SELECT @pipeline().parameters.Query;\n",
							"type": "Expression"
						},
						"exportSettings": {
							"type": "SnowflakeExportCopyCommand"
						}
					},
					"sink": {
						"type": "DelimitedTextSink",
						"storeSettings": {
							"type": "AzureBlobFSWriteSettings"
						},
						"formatSettings": {
							"type": "DelimitedTextWriteSettings",
							"quoteAllText": true,
							"fileExtension": ".txt"
						}
					},
					"enableStaging": false
				},
				"inputs": [
					{
						"referenceName": "DNA_CORE_Dataset",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "snowflake_adls_copy",
						"type": "DatasetReference",
						"parameters": {
							"src_path": {
								"value": "@pipeline().parameters.Path",
								"type": "Expression"
							},
							"src_container": {
								"value": "@pipeline().parameters.Container",
								"type": "Expression"
							},
							"src_filename": {
								"value": "@pipeline().parameters.FileName",
								"type": "Expression"
							}
						}
					}
				]
			}
		],
		"parameters": {
			"Src_ID": {
				"type": "string",
				"defaultValue": "1"
			},
			"Query": {
				"type": "string",
				"defaultValue": "select distinct mnth_id,data_src,sum(in_transit_val) in_transit_val,sum(jj_grs_trd_sls) jj_grs_trd_sls,sum(end_stock_val) end_stock_val  ,sum(trgt_val) trgt_val from os_edw.EDW_MY_SELLOUT_ANALYSIS where mnth_id>='202301' group by 1,2 order by 1 desc;--not matching for 2024(end_stock_val & jj_grs_trd_sls)"
			},
			"Container": {
				"type": "string",
				"defaultValue": "Dev"
			},
			"Path": {
				"type": "string",
				"defaultValue": "Dev\\Result"
			},
			"FileName": {
				"type": "string",
				"defaultValue": "Result"
			}
		},
		"folder": {
			"name": "Miscellaneous Pipelines"
		},
		"annotations": []
	}
}