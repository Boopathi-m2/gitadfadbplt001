{
	"name": "test",
	"properties": {
		"activities": [
			{
				"name": "Copy data1",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "SqlServerSource",
						"sqlReaderQuery": "SELECT ac_code, ac_attribute, sku_stockcode, sku_attribute, sku_profitcentre, est_date,\n       (SUM(est_estimate)- SUM(est_promotional)) as sum_est_normal, SUM(est_promotional) as sum_est_promotional,\n       SUM(est_estimate) as sum_est_estimate\nFROM account\nINNER JOIN estimate ON est_accountrowid = ac_rowid\nINNER JOIN sku ON est_skurowid = sku_rowid\nWHERE est_date >= DATEADD(MONTH, -42, getdate())\nGROUP BY ac_code, ac_attribute, sku_stockcode, sku_attribute, sku_profitcentre, est_date;",
						"queryTimeout": "02:00:00",
						"partitionOption": "None"
					},
					"sink": {
						"type": "DelimitedTextSink",
						"storeSettings": {
							"type": "AzureBlobFSWriteSettings"
						},
						"formatSettings": {
							"type": "DelimitedTextWriteSettings",
							"quoteAllText": true,
							"fileExtension": ".txt"
						}
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"typeConversion": true,
						"typeConversionSettings": {
							"allowDataTruncation": true,
							"treatBooleanAsNumber": false
						}
					}
				},
				"inputs": [
					{
						"referenceName": "sql_server_awswfqsgpw",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "sql_server_sink",
						"type": "DatasetReference",
						"parameters": {
							"container": "pac",
							"path": "dev/sql_server/test/",
							"file": "sku.csv"
						}
					}
				]
			}
		],
		"folder": {
			"name": "Miscellaneous Pipelines"
		},
		"annotations": []
	}
}