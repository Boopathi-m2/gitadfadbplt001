{
	"name": "test",
	"properties": {
		"activities": [
			{
				"name": "Copy data1",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "SqlServerSource",
						"sqlReaderQuery": "SELECT sku_apncode, CONVERT(numeric(18,0), nullif(sku_apncode,'')) AS APN , sku_stockcode\n                 FROM dbo.sku\n                 WHERE  sku_apncode <> '' ;",
