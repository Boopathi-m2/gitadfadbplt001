		"parameters": {
			"S3Bucket": {
				"type": "string",
				"defaultValue": "itx-arm-conapdna-aspac-qa"
			},
			"S3Directory": {
				"type": "string",
				"defaultValue": "raw-data-lake/travel_retail/transaction_files/archive"
