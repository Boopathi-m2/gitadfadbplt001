					"scripts": [
						{
							"type": "NonQuery",
							"text": {
								"value": "COPY INTO \r\n    @{variables('target_schema')}.@{variables('target_table')}\r\n     FROM @{concat('''@', variables('stage'), '/',variables('ftp_path'),'/''')}\r\n     FILE_FORMAT =(FORMAT_NAME = @{variables('format')})",
