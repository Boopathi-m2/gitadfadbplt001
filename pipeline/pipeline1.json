{
	"name": "pipeline1",
	"properties": {
		"activities": [
			{
				"name": "Copy data1",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "AmazonRdsForSqlServerSource",
						"sqlReaderQuery": "SELECT Tag,Period_type,TYPE,Period_Date,Brand,GFO,\"Need State\",Category,\"Sub-Category\",Segment,\"Sub-Segment\",Manufacturer,\"Country/Cluster\",Region,\"Market Type\",Cluster,\"Country (geo)\",\"Special Region\",\"Country Strategic Role\",Universe,\"Last Period\",Value,brand_manufacturer_flg,\"Database Category\",\"Database Date\",\"Text Desc\",Metric_Period,\"Update\",FileName,Channel FROM bpanorama.bpwarehouse.GBA_QSD_Market where region='APAC';",
						"queryTimeout": "02:00:00",
						"partitionOption": "None"
					},
					"sink": {
						"type": "DelimitedTextSink",
						"storeSettings": {
							"type": "AzureBlobFSWriteSettings"
						},
						"formatSettings": {
							"type": "DelimitedTextWriteSettings",
							"quoteAllText": true,
							"fileExtension": ".txt"
						}
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"typeConversion": true,
						"typeConversionSettings": {
							"allowDataTruncation": true,
							"treatBooleanAsNumber": false
						}
					}
				},
				"inputs": [
					{
						"referenceName": "AmazonRdsForCapturesqlrds",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "sql_server_sink",
						"type": "DatasetReference",
						"parameters": {
							"container": "asp",
							"path": "dev/Regional_Market_Share/",
							"file": "GBA_QSD_Market_data_extraction"
						}
					}
				]
			}
		],
		"annotations": []
	}
}