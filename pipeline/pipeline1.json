{
	"name": "pipeline1",
	"properties": {
		"activities": [
			{
				"name": "Set extracted date japan",
				"type": "SetVariable",
				"state": "Inactive",
				"onInactiveMarkAs": "Succeeded",
				"dependsOn": [
					{
						"activity": "Set variable1",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"policy": {
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"variableName": "extracted_object",
					"value": {
						"value": "@if(contains(variables('filename'),'OTHERS'),split(variables('filename'), '_')[2],split(variables('filename'),'_')[1])",
						"type": "Expression"
					}
				}
			},
			{
				"name": "Set variable1",
				"type": "SetVariable",
				"state": "Inactive",
				"onInactiveMarkAs": "Succeeded",
				"dependsOn": [],
				"policy": {
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"variableName": "filename",
					"value": "OTHERS_CIAN_20231204_1.csv"
				}
			},
			{
				"name": "Copy data SQL",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "OracleSource",
						"oracleReaderQuery": "SELECT  ID,RCV_DT,TEST_FLAG,BGN_SNDR_CD,WS_CD,RTL_TYPE,RTL_CD,TRADE_TYPE,SHP_DATE,SHP_NUM,TRADE_CD,DEP_CD,CHG_CD,PERSON_IN_CHARGE,PERSON_NAME,RTL_NAME,RTL_HO_CD,RTL_ADDRESS_CD,DATA_TYPE,OPT_FLD,ITEM_NM,ITEM_CD_TYP,ITEM_CD,QTY,QTY_TYPE,PRICE,PRICE_TYPE,BGN_SNDR_CD_GLN,RCV_CD_GLN,WS_CD_GLN,SHP_WS_CD,SHP_WS_CD_GLN,REP_NAME_KANJI,REP_INFO,TRADE_CD_GLN,RTL_CD_GLN,RTL_NAME_KANJI,RTL_HO_CD_GLN,ITEM_CD_GTIN,ITEM_NM_KANJI,UNT_PRC,NET_PRC,SALES_CHAN_TYPE\r\nFROM JPEDI.SELL_OUT_PLANET;",
						"partitionOption": "None",
						"queryTimeout": "02:00:00"
					},
					"sink": {
						"type": "DelimitedTextSink",
						"storeSettings": {
							"type": "AzureBlobFSWriteSettings"
						},
						"formatSettings": {
							"type": "DelimitedTextWriteSettings",
							"quoteAllText": true,
							"fileExtension": ".txt"
						}
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"typeConversion": true,
						"typeConversionSettings": {
							"allowDataTruncation": true,
							"treatBooleanAsNumber": false
						}
					}
				},
				"inputs": [
					{
						"referenceName": "OracleTable1",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "sql_server_sink",
						"type": "DatasetReference",
						"parameters": {
							"container": "jpn",
							"path": "dev/EDI_TABLES/master/",
							"file": "EDI_SELL_OUT.csv"
						}
					}
				]
			}
		],
		"variables": {
			"filename": {
				"type": "String"
			},
			"extract_name": {
				"type": "String"
			},
			"extract_value": {
